{% render 'countto-block', from: block.settings.from, to: block.settings.to, duration: block.settings.duration, delay: block.settings.delay %}

{% schema %}
{
  "name": "t:blocks.countto.name",
  "settings": [
    {
      "type": "number",
      "id": "from",
      "label": "t:blocks.countto.settings.from.label",
      "default": 0
    },
    {
      "type": "number",
      "id": "to",
      "label": "t:blocks.countto.settings.to.label",
      "default": 1000
    },
    {
      "type": "number",
      "id": "duration",
      "label": "t:blocks.countto.settings.duration.label",
      "default": 3000
    },
    {
      "type": "number",
      "id": "delay",
      "label": "t:blocks.countto.settings.delay.label",
      "default": 1000
    }
  ],
  "presets": [
    {
      "name": "t:blocks.countto.presets.name"
    }
  ]
}
{% endschema %}
