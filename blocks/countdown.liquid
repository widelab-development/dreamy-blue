{% render 'countdown-block', time: block.settings.time %}

{% schema %}
{
  "name": "t:blocks.countdown.name",
  "settings": [
    {
      "type": "text",
      "id": "time",
      "label": "t:blocks.countdown.settings.time.label",
      "info": "t:blocks.countdown.settings.time.info"
    }
  ],
  "presets": [
    {
      "name": "t:blocks.countdown.presets.name"
    }
  ]
}
{% endschema %}
